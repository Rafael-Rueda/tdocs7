{
    "name": "tdocs7",
    "version": "1.0.2",
    "description": "MCP Server para busca em documentação com RAG-like chunking",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "tdocs7": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "start": "node --env-file=.env dist/index.js",
        "dev": "tsx --env-file=.env src/index.ts",
        "lint": "biome lint src/",
        "format": "biome format --write src/",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "mcp",
        "documentation",
        "search",
        "rag",
        "claude"
    ],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=20.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.10",
        "@rueda.dev/config": "^1.6.1",
        "@types/node": "^22.15.0",
        "tsx": "^4.19.0",
        "typescript": "^5.8.0"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.25.1",
        "axios": "^1.13.2",
        "zod": "^4.2.1"
    }
}
